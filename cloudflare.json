{
    "name":"Secpanel",
    "category":"server security",
    "description": "Secpanel is a one click security tool for linux servers. Dedicated, VPS and cloud servers.",
    "version": "2.1.1",
	"contributors":[
	{
		"name":"Anand Kumar Mishra",
		"email":"akm@secpanel.com"
	},{
		"name":"Rudhir Sharan",
		"email":"rudhir@secpanel.com"
	}
	],
    "account": {
        "callback_url": "https://callbacks.secpanel.com/cfapis/",
        "user_fields": ["email"]
    },
    "billing": {
        "type": "zone",
        "plans":[
            {
                "name" : "Single Server Monthly Billing",
                "price": "29.95"
            }
        ]
    },
    "config":{
        "details":{
            "Price": "$29.95/month",
            "Category": "server security",
            "Restrictions": "None"
        },
        "assets":{
            "logos" : {
                "132px": "./public/images/logo-132.png",
                "200px": "./public/images/logo-200.png"
            },
            "detail_page" : [
                "./public/images/dashboard-mainview.jpg",
                "./public/images/dashboard-configurationpanelview.jpg"
            ]
        },
        "interface": [
            {
                "type": "string",
                "name": "Customer Name",
                "description": "Enter your full name",
                "id": "cname",
                "domain_request": true,
                "private": true
            },
            {
                "type": "string",
                "name": "Server IP",
                "description": "Enter the IPv4 IP address of your server.",
                "id": "server_ip",
                "domain_request": true,
                "private": true
            }
        ]
    }
}
